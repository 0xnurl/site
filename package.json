{
  "name": "nohello.net",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/nohello-net/nohello-net.github.io/"
  },
  "scripts": {
    "build": "eleventy",
    "serve": "eleventy --serve --port=8123",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "yarn test:ui && yarn test:unit",
    "test:ui": "playwright test test/ui",
    "test:ui:update": "yarn test:ui --update-snapshots",
    "test:unit": "mocha test/unit",
    "ci:ui": "start-server-and-test serve 8123 test:ui",
    "strings:push": "tx push",
    "strings:pull": "tx pull"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@playwright/test": "^1.18.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.2.0",
    "nunjucks": "^3.2.3",
    "prettier": "^2.3.2",
    "start-server-and-test": "^1.14.0"
  },
  "dependencies": {
    "eleventy-plugin-i18n-gettext": "^1.5.1",
    "marked": "^4.0.12"
  }
}
