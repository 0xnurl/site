{
  "name": "nohello.net",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/nohello-net/site/"
  },
  "scripts": {
    "build": "eleventy",
    "serve": "eleventy --serve --port=8123",
    "check-snapshots": "act -j check-snapshots -P ubuntu-latest=mcr.microsoft.com/playwright:v1.16.3 --bind -W ./.github/local-workflows --reuse",
    "update-snapshots": "act -j update-snapshots -P ubuntu-latest=mcr.microsoft.com/playwright:v1.16.3 --bind -W ./.github/local-workflows --reuse",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "yarn test:ui && yarn test:unit",
    "test:ui": "playwright test test/ui",
    "test:ui:update": "yarn test:ui --update-snapshots",
    "test:unit": "mocha test/unit",
    "ci:ui": "start-server-and-test serve 8123 test:ui",
    "ci:ui:update": "start-server-and-test serve 8123 test:ui:update",
    "strings:push": "tx push",
    "strings:pull": "tx pull",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@playwright/test": "^1.18.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.2.0",
    "nunjucks": "^3.2.3",
    "prettier": "^2.3.2",
    "start-server-and-test": "^1.14.0"
  },
  "dependencies": {
    "eleventy-plugin-i18n-gettext": "^1.5.1",
    "marked": "^4.0.12",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0"
  },
  "resolutions": {
    "@11ty/eleventy": "^1.0.0",
    "printf": "^0.6.1"
  }
}
